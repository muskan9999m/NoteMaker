<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>saveNoteServlet</servlet-name>
  	<display-name>saveNoteServlet</display-name>
  	<description></description>
  	<servlet-class>Servlets.saveNoteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>deleteServlet</servlet-name>
  	<display-name>deleteServlet</display-name>
  	<description></description>
  	<servlet-class>Servlets.deleteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>updateServlet</servlet-name>
  	<display-name>updateServlet</display-name>
  	<description></description>
  	<servlet-class>Servlets.updateServlet</servlet-class>
  	
  </servlet>
  <servlet-mapping>
  	<servlet-name>saveNoteServlet</servlet-name>
  	<url-pattern>/saveNoteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>deleteServlet</servlet-name>
  	<url-pattern>/deleteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>updateServlet</servlet-name>
  	<url-pattern>/updateServlet</url-pattern>
  </servlet-mapping>
</web-app>
